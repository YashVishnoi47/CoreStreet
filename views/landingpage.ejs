<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Landing Page</title>
    <link href="/css/style.css" rel="stylesheet" />
    <link href="/stylesheets/styling.css" rel="stylesheet" />
  </head>

  <body class="h-screen">

    <div id="flash-message" class="flash-message">
      <% if(success.length>0){ %>
      <div class="absolute top-5 z-50  right-2 p-3 rounded-md bg-blue-500">
        <span class="inline-block mt-1 mb-1 text-white"> <%= success %> </span>
      </div>
      <% } %> <% if(error.length>0){ %>
      <div
        class="absolute w-full min-h-full flex justify-center items-center shadow-blue-shadow transition-all duration-200 ease-in-out"
      >
        <div
          class="absolute flex flex-col gap-10 justify-center py-8 items-center h-auto border-4 border-blue-500 w-[40%] z-50 mt-20 mr-2 p-3 rounded-2xl bg-white"
        >
          <span class="inline-block font-semibold relative text-xl text-black">
            <%= error %> to continue Shopping
          </span>
          <div class="flex gap-4 w-full justify-center">
            <button
              class="error-flex-btn-remove px-5 rounded-full py-3 mt-2 bg-white w-[20%] text-black border-[0.5px] border-black hover:bg-red-600 hover:text-white cursor-pointer transition-all duration-200 ease-in-out"
            >
              Not now
            </button>
            <a
              href="/register"
              class="error-flex-btn-log-in px-5 rounded-full py-3 mt-2 bg-white w-[20%] text-black border-[0.5px] border-black flex justify-center items-center hover:bg-blue-600 hover:text-white cursor-pointer transition-all duration-200 ease-in-out"
              >Log in</a
            >
          </div>
        </div>
      </div>
      <% } %>
    </div>

    <div
      class="alert min-w-full relative gap-2 justify-center items-center text-white flex bg-blue-500 min-h-10 z-50"
    >
      <p class="alert-msg-1 font-bold min-h-full">End of Season Sale:</p>
      <p class="alert-msg-2 font-bold min-h-full">
        <a class="underline" href="/shop">Shop</a> your favorite styles at up to
        50% OFF!
      </p>
      <!-- <a href="/shop" class="alert-msg-3 font-extrabold underline">Shop now</a> -->
    </div>

    <div
      class="navbar border-b-2 border-blue-100 z-50 absolute h-[5rem] flex min-w-full"
    >
      <div class="left relative flex items-center h-full w-[50%]">
        <h1
          class="landing-logo font-semibold text-blue-50 select-none cursor-pointer text-2xl p-2 flex gap-0"
        >
          Core<span class="text-blue-100 logo font-semibold landing-logo"
            >Street</span
          >
        </h1>
      </div>

      <div class="right relative h-full w-[50%]">
        <div class="nav-links flex gap-4 justify-end items-center h-full p-3">
          <div
            class="border-2 top-1 hover:bg-blue-200 flex transition-all duration-150 ease-in-out cursor-pointer justify-center items-center w-10 rounded-full bg-white h-10 border-white"
          >
            <a
              id="nav-home"
              class="text-2xl scale-100 transition-all duration-150 ease-in-out z-50 relative"
              href="/"
              ><img src="/images/home.svg" alt=""
            /></a>
          </div>

          <div
            class="border-2 top-1 hover:bg-blue-200 flex transition-all duration-150 ease-in-out cursor-pointer justify-center items-center w-10 rounded-full bg-white h-10 border-white"
          >
            <a
              id="nav-btn-2"
              class="text-2xl scale-100 transition-all duration-150 ease-in-out z-50 relative"
              href="/shop"
              ><img src="/images/shop.svg" alt=""
            /></a>
          </div>

          <!-- logout -->
          <!-- <a id="nav-logout"
                            class="px-5 flex justify-center items-centern gap-2 rounded-full  py-3  bg-blue-500 m-w-[50%] text-white hover:bg-blue-600 cursor-pointer transition-all duration-200 ease-in-out"
                            href="/users/logout">Logout <img src="/images/logout.svg" alt="" /></a> -->

          <!-- Content for users not logged in -->
          <!-- <a id="nav-btn-1"
                                    class="rounded-full min-w-20 flex justify-center items-center py-3 mt-1 bg-white text-blue-500 hover:bg-blue-100 hover:text-black cursor-pointer transition-all duration-200 ease-in-out"
                                    href="/register">Log in</a>
                                <a id="nav-btn-1"
                                    class="rounded-full min-w-20 flex justify-center items-center py-3 mt-1 bg-white text-blue-500 hover:bg-blue-100 hover:text-black cursor-pointer transition-all duration-200 ease-in-out"
                                    href="/register">Sign up</a> -->
        </div>
      </div>
      <div id="menu-btn" class="menu-btn right-2 top-4 z-50 absolute">
        <img
          id="nav-menu"
          class="nav-menu w-8 h-8 cursor-pointer fill-white"
          src="/images/menu.svg"
          alt=""
        />
      </div>
    </div>

    <div
      id="res-nav"
      class="res-nav transition-all duration-200 ease-in-out z-50 items-center justify-center w-full relative left-5 top-0"
    >
      <div
        class="res-nav-in p-6 flex flex-col gap-4 h-auto min-w-[90%] border-2 top-4 border-blue-500 absolute"
      >
        <!-- Home -->
        <a
          id="nav-btn-1"
          class="px-5 rounded-full py-3 mt-2 bg-blue-500 w-[100%] text-white hover:bg-blue-600 cursor-pointer transition-all duration-200 ease-in-out"
          href="/home"
          >Home</a
        >

        <a
          id="nav-btn-2"
          class="px-5 rounded-full py-3 mt-2 bg-blue-500 w-[100%] text-white hover:bg-blue-600 cursor-pointer transition-all duration-200 ease-in-out"
          href="/cart"
          >Cart</a
        >
      </div>
    </div>

    <div class="slide-container w-full relative top-0 overflow-hidden">
      <!-- <div class=" w-full h-full absolute flex flex-col justify-center items-center">
            <h1 class=" slide-text-1 text-white text-9xl z-20 w-10    ">Wear Your Confidence.</h1>
            <h1 class=" slide-text-2 text-white  text-3xl z-20 font-bold   ">Where style meets comfort, every stitch tells your story.</h1>
        </div> -->
      <div
        class="blackcover w-full h-full absolute flex justify-normal items-center bg-black opacity-40"
      ></div>
      <div class="slider flex">
        <img
          class="banner-image w-full h-auto object-cover"
          src="/images/Winter Collection-2.jpg"
          alt="Image 1"
        />
      </div>
    </div>

    <div
      class="shoping-cards flex relative flex-col items-center mt-4 gap-10 w-full min-h-[600px]"
    >
      <h1 class="text-4xl font-medium">Our Top Products</h1>

      <div class="main-landing-div-1 mt-4 flex flex-col gap-5">
        <div class="flex flex-wrap main-div-2 items-start gap-4">
          <% if (products.length> 0) { %>
          <a href="/product/selectedproduct/<%= products[0]._id %>">
            <!-- Main product card -->
            <div
              class="main-cloth-shop relative w-72 h-[410px] hover:shadow-black-shadow transition-all duration-200 ease-in-out flex flex-col gap-1"
            >
              <!-- Product Image -->
              <div
                class="w-full relative h-[70%] flex items-center justify-center bg-[<%= products[0].bgcolor %>]"
              >
                <img
                  class="h-full product-image scale-100 transition-all duration-200 ease-in-out"
                  src="data:image/jpeg;base64,<%= products[0].image.toString('base64') %>"
                  alt=""
                />

                <!-- Add to cart button -->
                <a
                  class="add-to-cart-btn absolute flex justify-center items-center px-5 rounded-full py-2 bg-white w-[90%] text-blue-500 hover:bg-blue-200 hover:text-black opacity-0 cursor-pointer transition-all duration-200 ease-in-out bottom-2"
                  href="/addtocart/<%= products[0]._id %>"
                >
                  <p>Add to cart</p>
                </a>
              </div>
              <!-- Product Details (Panel) -->
              <div
                class="flex justify-between relative top-3 bg-[<%= products[0].panecolor %>] items-center px-2 text-[<%= products[0].textcolor %>]"
              >
                <div>
                  <h3 class="font-medium"><%= products[0].name %></h3>

                  <h4 class="font-semibold">₹ <%= products[0].price %></h4>
                </div>
              </div>
            </div>
          </a>
          <% } else { %>
          <p>No products available</p>
          <% } %> <% if (products.length> 0) { %>
          <a href="/product/selectedproduct/<%= products[1]._id %>">
            <!-- Main product card -->
            <div
