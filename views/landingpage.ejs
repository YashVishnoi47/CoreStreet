                  href="/addtocart/<%= products[1]._id %>"
                >
                  <p>Add to cart</p>
                </a>
              </div>
              <!-- Product Details (Panel) -->
              <div
                class="flex justify-between relative top-3 bg-[<%= products[1].panecolor %>] items-center px-2 text-[<%= products[1].textcolor %>]"
              >
                <div>
                  <h3 class="font-medium"><%= products[1].name %></h3>

                  <h4 class="font-semibold">₹ <%= products[1].price %></h4>
                </div>
              </div>
            </div>
          </a>
          <% } else { %>
          <p>No products available</p>
          <% } %> <% if (products.length> 0) { %>
          <a href="/product/selectedproduct/<%= products[2]._id %>">
            <!-- Main product card -->
            <div
              class="main-cloth-shop relative w-72 h-[410px] hover:shadow-black-shadow transition-all duration-200 ease-in-out flex flex-col gap-1"
            >
              <!-- Product Image -->
              <div
                class="w-full relative h-[70%] flex items-center justify-center bg-[<%= products[2].bgcolor %>]"
              >
                <img
                  class="h-full product-image scale-100 transition-all duration-200 ease-in-out"
